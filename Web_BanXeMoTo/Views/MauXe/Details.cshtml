@model Web_BanXeMoTo.Controllers.ViewModel

@{
    ViewData["Title"] = "Chi tiết mẫu xe";
    Layout = "~/Views/Shared/_LayoutManager.cshtml";
}

<div class="app-main__inner">
    <div class="app-page-title">
        <div class="page-title-wrapper">
            <div class="page-title-heading">
                <div>

                    <h2 style="font-weight:500">Quản lý Mẫu xe</h2>

                </div>
            </div>
        </div>
    </div>
    <ul class="body-tabs body-tabs-layout tabs-animated body-tabs-animated nav">
        <li class="nav-item">
            <a asp-action="Index" class="nav-link">
                <span>Mẫu xe</span>
            </a>
        </li>
    </ul>
    <div class="row">
        <div class="col-md-10">
            <div class="main-card mb-3 card">
                <div class="card-body">
                    <div>
                        <partial name="_ListDetails" model="Model"/>
                    </div>
                    <hr />
                    <div class="form-group">
                        <a asp-action="Edit" asp-route-id="@Model.mauXe.Idmau" class="btn btn-sm btn-outline-warning">Sửa</a>
                        <a asp-action="Index" class="btn btn-sm btn-outline-secondary">Trở về</a>
                    </div>
                </div>
        </div>
    </div>
</div>
